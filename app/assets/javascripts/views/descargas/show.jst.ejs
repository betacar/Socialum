<article class="descarga row-fluid" data-bax-id="<%= bax_id %>" data-gabarra-id="<%= gabarra_id %>" id="descarga-<%= bax_id %>-<%= gabarra_id %>">
  <span class="fa-icon-remove-sign" title="Cerrar ventana de descarga" id="cerrar-descarga-<%= bax_id %>-<%= gabarra_id %>"></span>

  <section class="phi-side">
    <h5><%= gabarra_id %></h5>
    <div class="analisis row">
      <div class="span6">
        <h6>peso</h6>
        <p><%= parseFloat(tone_transportada).format(2, '.', ',') %> tons.</p>

        <h6>al<sub>2</sub>o<sub>2</sub></h6>
        <p><%= al2o3.replace(/\./, ',') %>%</p>

        <h6>s. reactivo</h6>
        <p><%= sio2r.replace(/\./, ',') %>%</p>

        <h6>muestreo</h6>
        <p><%= EJS.Helpers.prototype.muestreo(muestreo) %></p>
      </div>

      <div class="span6 alpha">
        <h6>humedad</h6>
        <p><%= humedad.replace(/\./, ',') %>%</p>

        <h6>fe<sub>2</sub>o<sub>3</sub></h6>
        <p><%= fe2o3.replace(/\./, ',') %>%</p>

        <h6>s. cuarzo</h6>
        <p><%= sio2q.replace(/\./, ',') %>%</p>

        <h6>procedencia</h6>
        <p><%= EJS.Helpers.prototype.procedencia(tipo_carga) %></p>
      </div>
    </div>

