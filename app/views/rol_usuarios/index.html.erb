<h1>Listing rol_usuarios</h1>

<table>
  <tr>
    <th>Estado</th>
    <th>Usuario</th>
    <th>Rol</th>
    <th>Usuario id created</th>
    <th>Usuario id updated</th>
  </tr>

<% @rol_usuarios.each do |rol_usuario| %>
  <tr>
    <td><%=h rol_usuario.estado %></td>
    <td><%=h rol_usuario.usuario_id %></td>
    <td><%=h rol_usuario.rol %></td>
    <td><%=h rol_usuario.usuario_id_created %></td>
    <td><%=h rol_usuario.usuario_id_updated %></td>
    <td><%= link_to 'Show', rol_usuario %></td>
    <td><%= link_to 'Edit', edit_rol_usuario_path(rol_usuario) %></td>
    <td><%= link_to 'Destroy', rol_usuario, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New rol_usuario', new_rol_usuario_path %>