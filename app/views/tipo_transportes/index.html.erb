<h1>Listing tipo_transportes</h1>

<table>
  <tr>
    <th>Estado</th>
    <th>Desc tipo transporte</th>
    <th>Usuario id created</th>
    <th>Usuario id update</th>
  </tr>

<% @tipo_transportes.each do |tipo_transporte| %>
  <tr>
    <td><%=h tipo_transporte.estado %></td>
    <td><%=h tipo_transporte.desc_tipo_transporte %></td>
    <td><%=h tipo_transporte.usuario_id_created %></td>
    <td><%=h tipo_transporte.usuario_id_update %></td>
    <td><%= link_to 'Show', tipo_transporte %></td>
    <td><%= link_to 'Edit', edit_tipo_transporte_path(tipo_transporte) %></td>
    <td><%= link_to 'Destroy', tipo_transporte, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New tipo_transporte', new_tipo_transporte_path %>