<h1>Listing punto_tolvas</h1>

<table>
  <tr>
    <th>Estado</th>
    <th>Desc punto tolva</th>
    <th>Minimo punto tolva</th>
    <th>Usuario id created</th>
    <th>Usuario id update</th>
  </tr>

<% @punto_tolvas.each do |punto_tolva| %>
  <tr>
    <td><%=h punto_tolva.estado %></td>
    <td><%=h punto_tolva.desc_punto_tolva %></td>
    <td><%=h punto_tolva.minimo_punto_tolva %></td>
    <td><%=h punto_tolva.usuario_id_created %></td>
    <td><%=h punto_tolva.usuario_id_update %></td>
    <td><%= link_to 'Show', punto_tolva %></td>
    <td><%= link_to 'Edit', edit_punto_tolva_path(punto_tolva) %></td>
    <td><%= link_to 'Destroy', punto_tolva, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

