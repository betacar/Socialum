<% form_for(@stock_gabarra) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :estado %><br />
    <%= f.text_field :estado %>
  </p>
  <p>
    <%= f.label :locacion %><br />
    <%= f.text_field :locacion %>
  </p>
  <p>
    <%= f.label :estatus_gabarra %><br />
    <%= f.text_field :estatus_gabarra %>
  </p>
  <p>
    <%= f.label :cod_gabarra %><br />
