<h1>Listing tipo_materias</h1>

<table>
  <tr>
    <th>Estado</th>
    <th>Desc tipo materia</th>
    <th>Usuario id created</th>
    <th>Usuario id updated</th>
  </tr>

<% @tipo_materias.each do |tipo_materia| %>
  <tr>
    <td><%=h tipo_materia.estado %></td>
    <td><%=h tipo_materia.desc_tipo_materia %></td>
    <td><%=h tipo_materia.usuario_id_created %></td>
    <td><%=h tipo_materia.usuario_id_updated %></td>
    <td><%= link_to 'Show', tipo_materia %></td>
    <td><%= link_to 'Edit', edit_tipo_materia_path(tipo_materia) %></td>
    <td><%= link_to 'Destroy', tipo_materia, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New tipo_materia', new_tipo_materia_path %>