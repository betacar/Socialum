<h1>Listing estatus_gabarras</h1>

<table>
  <tr>
    <th>Estado</th>
    <th>Desc estatus gabarra</th>
    <th>Usuario id created</th>
    <th>Usuario id updated</th>
  </tr>

<% @estatus_gabarras.each do |estatus_gabarra| %>
  <tr>
    <td><%=h estatus_gabarra.estado %></td>
    <td><%=h estatus_gabarra.desc_estatus_gabarra %></td>
    <td><%=h estatus_gabarra.usuario_id_created %></td>
    <td><%=h estatus_gabarra.usuario_id_updated %></td>
    <td><%= link_to 'Show', estatus_gabarra %></td>
    <td><%= link_to 'Edit', edit_estatus_gabarra_path(estatus_gabarra) %></td>
    <td><%= link_to 'Destroy', estatus_gabarra, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

